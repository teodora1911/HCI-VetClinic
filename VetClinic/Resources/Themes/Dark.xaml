<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="ForegroundColor">#EEEEEE</SolidColorBrush>
    <SolidColorBrush x:Key="ForegroundNegativeColor">#121212</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryColor">#BB86FC</SolidColorBrush>
    <SolidColorBrush x:Key="SecondaryColor">#03DAC6</SolidColorBrush>
    <SolidColorBrush x:Key="PrimaryBackgroundColor">#222831</SolidColorBrush>
    <SolidColorBrush x:Key="SecondaryBackgroundColor">#393E46</SolidColorBrush>
    <SolidColorBrush x:Key="ErrorColor">#CF6679</SolidColorBrush>

    <Style x:Key="FontFamilyWindow" TargetType="{x:Type Window}">
        <Setter Property="FontFamily" Value="Times New Roman" />
        <Setter Property="FontSize" Value="18" />
    </Style>
</ResourceDictionary>